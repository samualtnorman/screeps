{
	"compilerOptions": {
		"module": "ESNext",
		"lib": [ "ESNext" ],
		"target": "es2017",
		"moduleResolution": "Node",
		"outDir": "dist",
		"sourceMap": true,
		"strict": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"noFallthroughCasesInSwitch": true
	}
}
